Game of marbles
from math import gcd
try:
    n=int(input())
    n=n//2
    markD=0
    markS=0
    q=[]
    for i in range(n):
        l=list(input().split())
        answer=list(input().split())
        questioner=l[0]
        q.append(questioner)
        clue=list(map(int,l[1].split(",")))
        print(f"{questioner}'s question is: {l[1]}")
        ans=clue[0]
        for i in clue[1:]:
            ans=int(ans*i/gcd(ans, i))
        ans=str(ans)
        if questioner=="Sally":
            if answer[-1]==ans:
                print("Correct Answer")
                print("Darrell: 10points")
                markD+=10
            elif answer[-1]=="PASS":
                print("Question is PASSed")
                print(f"Answer is: {ans}")
                print("Darrell: 0points")
            else:
                print("Wrong Answer")
                print("Darrell: 0points")
        elif questioner=="Darrell":
            if answer[-1]==ans:
                print("Correct Answer")
                print("Sally: 10points")
                markS+=10
            elif a[-1]=="PASS":
                print("Question is PASSed")
                print(f"Answer is: {ans}")
                print("Sally: 0points")
            else:
                print("Wrong Answer")
                print("Sally: 0points")
    print("Total Points:")
    if q[0]=="Darrell":
        print(f"{q[0]}: {markD}points")
        print(f"Sally: {markS}points")
    else:
        print(f"Sally: {markS}points")
        print(f"Darrell: {markD}points")
    if markS>markD:
        print("Game Result:Sally is winner")
    elif markS==markD:
        print("Game Result: Draw")
    else:
        print("Game Result:Darrell is winner")
except:
    print("Invalid Input")
    
    
