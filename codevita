from itertools import permutations as p
n,m=map(int,input().split())
a=str(n)
b=str(m)
res=-1
pl=[]
if len(a)<len(b):
    print(res)
else:
    pl.extend(p(a))
    print(sorted(pl))
    pl.sort()
    for i in pl:
        i=int(''.join(i))
        if i>m:
            print(i)
            break
    else:
        print(res)
        
